DATABASE_URL=postgresql://localhost:5432/sealmeet

# SuiMeet Package ID (deployed contract address on Sui)
# Replace this with your actual deployed package ID
SUIMEET_PACKAGE_ID=0xc90c891397b21b051bd8f538f3b7daa087d185941aceb7b0dcd7b5c7a67be48f

# Sui RPC Configuration
# Mainnet: https://fullnode.mainnet.sui.io:443
# Testnet: https://fullnode.testnet.sui.io:443
# Devnet: https://fullnode.devnet.sui.io:443
SUI_RPC_URL=https://fullnode.testnet.sui.io:443

# Ingestion Configuration
# Maximum size of checkpoint backlog
CHECKPOINT_BUFFER_SIZE=5000

# Maximum number of checkpoints to fetch concurrently
INGEST_CONCURRENCY=200

# Polling interval to retry fetching checkpoints (milliseconds)
RETRY_INTERVAL_MS=200

# Indexer Cluster Configuration
# First checkpoint to start indexing from (0 = from genesis)
FIRST_CHECKPOINT=261705413

# Number of concurrent write operations to the database
WRITE_CONCURRENCY=10

# Logging Configuration
RUST_LOG=info,suimeet_indexer=debug,sui_indexer_alt_framework=info